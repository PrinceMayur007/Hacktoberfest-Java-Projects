<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="rootGridPane" style="-fx-background-color: #D9F7F0;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exploreandshare.example.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.9" prefHeight="30.0" />
        <RowConstraints />
    </rowConstraints>
    <Pane GridPane.columnSpan="2" />
    <Pane fx:id="insertedDiscPane" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="1" />
    <VBox style="-fx-background-color: #2B3B4C;" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <Region minHeight="-Infinity" minWidth="-Infinity" prefHeight="22.0" prefWidth="228.0" />
        <TextField fx:id="playerOneTextField" promptText="Player One">
            <VBox.margin>
                <Insets left="10.0" right="10.0" />
            </VBox.margin>
        </TextField>
        <TextField fx:id="playerTwoTextField" promptText="Player Two">
            <VBox.margin>
                <Insets bottom="5.0" left="10.0" right="10.0" />
            </VBox.margin>
        </TextField>
        <Button fx:id="setNamesButton" mnemonicParsing="false" prefHeight="29.0" prefWidth="248.0" text="Set Names">
            <VBox.margin>
                <Insets left="10.0" right="10.0" />
            </VBox.margin>
        </Button>
        <Region minHeight="-Infinity" minWidth="-Infinity" prefHeight="58.0" prefWidth="268.0" />
        <Label fx:id="playerTurnLabel" alignment="CENTER" prefHeight="46.0" prefWidth="268.0" textFill="WHITE">
            <font>
                <Font name="System Bold" size="34.0" />
            </font>
        </Label>
        <Label alignment="CENTER" prefHeight="28.0" prefWidth="267.0" text="TURN" textFill="WHITE">
            <font>
                <Font size="23.0" />
            </font>
        </Label>
        <Region prefHeight="137.0" prefWidth="261.0" />
    </VBox>
</GridPane>
